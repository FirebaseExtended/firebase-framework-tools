{
    "name": "@apphosting/adapter-astro",
    "version": "4.8.0",
    "main": "dist/index.js",
    "description": "Framework adapter to support astro in Firebase App Hosting",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/FirebaseExtended/firebase-framework-tools.git"
    },
    "bin": {
      "apphosting-adapter-astro-build": "dist/bin/build.js"
    },
    "author": {
      "name": "Firebase",
      "url": "https://firebase.google.com/"
    },
    "bugs": {
      "url": "https://github.com/FirebaseExtended/firebase-framework-tools/issues"
    },
    "type": "module",
    "sideEffects": false,
    "scripts": {
      "build": "rm -rf dist && tsc && chmod +x ./dist/bin/*",
      "test": "ts-mocha -p tsconfig.json src/**/*.spec.ts",
      "registry:local": "npx verdaccio --config ../publish-dev/verdaccio-config.yaml",
      "publish:local": "npm unpublish --@apphosting:registry=http://localhost:4873 --force && npm publish --@apphosting:registry=http://localhost:4873"
    },
    "exports": {
      ".": {
        "node": "./dist/index.js",
        "default": null
      },
      "./dist/*": {
        "node": "./dist/*",
        "default": null
      }
    },
    "files": [
      "dist"
    ],
    "license": "Apache-2.0",
    "dependencies": {
      "fs-extra": "^11.1.1",
      "yaml": "^2.3.4",
      "semver": "*"
    },
    "peerDependencies": {
      "next": "~14.0.0"
    },
    "peerDependenciesMeta": {
      "next": {
        "optional": true
      }
    },
    "devDependencies": {
      "@types/fs-extra": "*",
      "@types/tmp": "*",
      "next": "~14.0.0",
      "tmp": "*",
      "ts-node": "*",
      "verdaccio": "^5.30.3"
    }
  }
  